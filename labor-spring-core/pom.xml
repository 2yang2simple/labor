	<groupId>com.labor.spring.core</groupId>
	<artifactId>labor-spring-core</artifactId>
	<name>labor-spring-core</name>
	<packaging>jar</packaging>
	
	<properties>
		<labor.version>1.0.0</labor.version>
		<labor-common.version>1.0.0</labor-common.version>
