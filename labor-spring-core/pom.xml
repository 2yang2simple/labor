		<dependency>
			<groupId>com.labor.common</groupId>
			<artifactId>labor-common</artifactId>
			<version>${labor-common.version}</version>
		</dependency>
		<dependency>
			<groupId>com.labor.spring.feign</groupId>
			<artifactId>labor-spring-feign</artifactId>
			<version>${labor-spring-feign.version}</version>
		</dependency>
		
		<!-- spring boot -->
		
		<dependency>
		    <groupId>org.springframework.boot</groupId>
		    <artifactId>spring-boot-starter-cache</artifactId>
		</dependency>
		<!-- ehcache 	
		<dependency>
		    <groupId>net.sf.ehcache</groupId>
		    <artifactId>ehcache</artifactId>
		</dependency>-->
